{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Version information response schema.",
  "properties": {
    "api_version": {
      "description": "API version",
      "title": "Api Version",
      "type": "string"
    },
    "build_commit": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Git commit hash",
      "title": "Build Commit"
    },
    "build_time": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Build timestamp",
      "title": "Build Time"
    },
    "service": {
      "description": "Service name",
      "title": "Service",
      "type": "string"
    },
    "version": {
      "description": "Semantic version string",
      "title": "Version",
      "type": "string"
    }
  },
  "required": [
    "service",
    "version",
    "api_version"
  ],
  "title": "VersionResponse",
  "type": "object"
}
